{
  "name": "@localizer/translate",
  "version": "1.0.0",
  "description": "Type-safe localization, formatting and translation library. Provides utilities for TypeScript-first translation dictionaries.",
  "keywords": [
    "localization",
    "globalization",
    "i18n",
    "l10n",
    "internationalization",
    "localizer",
    "formatting",
    "translation"
  ],
  "license": "Apache-2.0",
  "author": "Artem Godin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/124c4a/localizer.git",
    "directory": "packages/translate"
  },
  "bugs": {
    "url": "https://github.com/124c4a/localizer/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@localizer/core": "1.0.0",
    "@localizer/format": "1.0.0",
    "tslib": "^2.3.0"
  },
  "nx": {
    "name": "@localizer/translate"
  }
}
