{
  "name": "@localizer/all",
  "version": "1.0.6",
  "description": "Type-safe localization, formatting and translation library. All @localizer components packaged together.",
  "keywords": [
    "localization",
    "globalization",
    "i18n",
    "l10n",
    "internationalization",
    "localizer",
    "formatting",
    "translation"
  ],
  "license": "Apache-2.0",
  "author": "Artem Godin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/124c4a/localizer.git",
    "directory": "packages/all"
  },
  "bugs": {
    "url": "https://github.com/124c4a/localizer/issues"
  },
  "homepage": "https://uselocalizer.dev",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@localizer/root": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@localizer/core": "1.2.1",
    "@localizer/format": "1.1.3",
    "@localizer/format-datetime": "1.1.3",
    "@localizer/format-displayname": "1.1.3",
    "@localizer/format-list": "1.1.3",
    "@localizer/format-lookup": "1.0.5",
    "@localizer/format-number": "1.1.3",
    "@localizer/format-relativetime": "1.1.3",
    "@localizer/transform": "1.1.3",
    "@localizer/translate": "1.2.1"
  }
}
