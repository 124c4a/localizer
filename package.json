{
  "name": "@localizer/root",
  "version": "0.0.0",
  "scripts": {},
  "private": true,
  "type": "module",
  "devDependencies": {
    "@babel/core": "7.28.0",
    "@babel/preset-react": "7.27.1",
    "@css-render/vue3-ssr": "0.15.14",
    "@eslint/js": "9.30.1",
    "@microsoft/api-extractor": "7.52.8",
    "@microsoft/eslint-formatter-sarif": "3.1.0",
    "@microsoft/sarif-multitool": "4.5.4",
    "@nx/eslint": "21.2.3",
    "@nx/eslint-plugin": "21.2.3",
    "@nx/js": "21.2.3",
    "@nx/react": "21.2.3",
    "@nx/vite": "21.2.3",
    "@nx/vue": "21.2.3",
    "@nx/web": "21.2.3",
    "@nx/workspace": "21.2.3",
    "@shikijs/vitepress-twoslash": "3.7.0",
    "@swc-node/register": "1.10.10",
    "@swc/core": "1.12.11",
    "@swc/helpers": "0.5.17",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@types/node": "22.15.35",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vicons/ionicons5": "0.13.0",
    "@vitejs/plugin-react": "4.6.0",
    "@vitejs/plugin-vue": "6.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.30.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-headers": "1.3.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-sonarjs": "3.0.4",
    "execa": "9.6.0",
    "gray-matter": "4.0.3",
    "jiti": "2.4.2",
    "jsdom": "26.1.0",
    "jsonc-eslint-parser": "2.4.0",
    "listr2": "9.0.1",
    "naive-ui": "2.42.0",
    "node-sarif-builder": "3.2.0",
    "nx": "21.2.3",
    "prettier": "3.6.2",
    "prettier-plugin-jsdoc": "1.3.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tslib": "2.8.1",
    "typedoc": "0.28.7",
    "typedoc-plugin-frontmatter": "1.3.0",
    "typedoc-plugin-markdown": "4.7.0",
    "typedoc-plugin-mdn-links": "5.0.3",
    "typedoc-vitepress-theme": "1.1.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.36.0",
    "undici": "7.11.0",
    "verdaccio": "6.1.5",
    "vite": "6.3.5",
    "vitepress": "1.6.3",
    "vitepress-sidebar": "1.32.1",
    "vitest": "3.2.4",
    "vue": "3.5.17",
    "vue-tsc": "3.0.1",
    "xlsx": "file:scripts/vendor/xlsx-0.20.3.tgz",
    "yaml": "2.8.0"
  },
  "workspaces": [
    "packages/*",
    "scripts/*"
  ],
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      },
      "typedoc": {
        "executor": "nx:run-commands",
        "cache": true,
        "options": {
          "commands": [
            "typedoc",
            "node scripts/reprocess-api-reference.js"
          ],
          "parallel": false
        },
        "dependsOn": [
          "^build"
        ],
        "inputs": [
          "{workspaceRoot}/packages/*/src/**/*.ts",
          "{workspaceRoot}/typedoc.base.json",
          "{workspaceRoot}/typedoc.json"
        ],
        "outputs": [
          "{workspaceRoot}/docs/api"
        ]
      },
      "build": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "typedoc"
        ],
        "options": {
          "commands": [
            "node scripts/generate-package-reference.js",
            "node scripts/generate-entity-reference.js",
            "node scripts/generate-card-reference.js",
            "node scripts/generate-recent-changes.js",
            "vitepress build"
          ]
        }
      },
      "dev": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "typedoc"
        ],
        "options": {
          "commands": [
            "node scripts/generate-package-reference.js",
            "node scripts/generate-entity-reference.js",
            "node scripts/generate-card-reference.js",
            "node scripts/generate-recent-changes.js",
            "vitepress dev"
          ]
        }
      },
      "preview": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "build"
        ],
        "options": {
          "commands": [
            "vitepress serve"
          ]
        }
      }
    }
  },
  "dependencies": {
    "@localizer/all": "*",
    "@localizer/x-vue": "*",
    "@localizer/x-react": "*"
  }
}
